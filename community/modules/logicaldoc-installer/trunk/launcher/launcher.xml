<launch4jConfig>
    <headerType>gui</headerType>
    <jar>logicaldoc-installer.jar</jar>
    <dontWrapJar>true</dontWrapJar>
    <outfile>setup.exe</outfile>
    <errTitle>LogicalDOC Community</errTitle>
    <chdir>.</chdir>
    <icon>../src/dist/bin/App32.ico</icon>
    <restartOnCrash>false</restartOnCrash>
    <jre>
        <minVersion>1.7.0</minVersion>
    </jre>
    <versionInfo>
        <fileVersion>VERSION.0</fileVersion>
        <txtFileVersion>VERSION</txtFileVersion>
        <fileDescription>LogicalDOC Community Installer</fileDescription>
        <copyright>Copyright &#169; YEAR LogicalDOC</copyright>
        <productVersion>VERSION.0</productVersion>
        <txtProductVersion>VERSION</txtProductVersion>
        <productName>LogicalDOC Community</productName>
        <companyName>LogicalDOC</companyName>
        <internalName>setup</internalName>
        <originalFilename>setup.exe</originalFilename>
    </versionInfo>
</launch4jConfig>