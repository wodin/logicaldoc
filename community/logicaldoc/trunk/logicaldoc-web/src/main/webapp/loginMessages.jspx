<?xml version = '1.0'?>
<!-- 
 Login messages page
  
 @author Marco Meschieri - Logical Objects
 @since 3.0
-->
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:ice="http://www.icesoft.com/icefaces/component"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:c="http://java.sun.com/jstl/core"
	xmlns:fn="http://java.sun.com/jsp/jstl/functions" jsfc="ice:panelGroup">
	<f:loadBundle basename="com.logicaldoc.web.i18n.application" var="msgs" />

	<br />
	<ice:panelGroup style="center" rendered="#{loginForm.notInitialized}">
		<ice:outputText styleClass="loginMessage">#{msgs['setup.reminder']}</ice:outputText>&amp;nbsp;
		<ice:outputLink styleClass="system_link" value="#{facesContext.externalContext.requestContextPath}/setup/">setup</ice:outputLink>
	</ice:panelGroup>
</jsp:root>
