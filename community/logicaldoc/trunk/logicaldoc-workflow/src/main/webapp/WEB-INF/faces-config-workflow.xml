<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config SYSTEM "web-facesconfig_1_1.dtd">
<faces-config xmlns="http://java.sun.com/JSF/Configuration">
 

  <!-- Home bean -->
  <managed-bean>
    <managed-bean-name>workflowTemplateManagerBean</managed-bean-name>
    <managed-bean-class>com.logicaldoc.workflow.editor.WorkflowTemplateManager</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
     <managed-property>
      <property-name>workflowTemplateLoader</property-name>
      <value>#{WorkflowTemplateLoader}</value>
    </managed-property>
  </managed-bean>
  
  <managed-bean>
    <managed-bean-name>workflowTemplateLoader</managed-bean-name>
    <managed-bean-class>com.logicaldoc.workflow.editor.WorkflowTemplateLoader</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
    
  </managed-bean>
	
  <managed-bean>
   
    <managed-bean-name>workflowBean</managed-bean-name>
    <managed-bean-class>com.logicaldoc.workflow.WorkflowManager</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
    
  </managed-bean>
	
   <navigation-rule>
    <from-view-id>*</from-view-id>
    <navigation-case>
      <from-outcome>startworkflowwizard</from-outcome>
      <to-view-id>/templates/workflow/wizard.iface</to-view-id>
      <redirect />
    </navigation-case>
   </navigation-rule>
</faces-config>
