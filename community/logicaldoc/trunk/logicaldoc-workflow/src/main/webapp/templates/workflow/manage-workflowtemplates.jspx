					<f:selectItems
						value="#{WorkflowTemplateManager.availableWorkflowTemplates}" />
				</ice:selectOneMenu>
				
				
			</td>
			<td style="background-image:url('/skins/default/images/workflow/editor/menu_gradient.jpg');">
				<ice:panelGroup>
					
					<ice:panelGroup styleClass="rightToolSitePanel">
						<ice:commandLink action="#{WorkflowTemplateManager.loadWorkflowTemplate}">
							<ice:graphicImage styleClass="linkImage" value="/skins/default/images/workflow/editor/load.gif"/>
							<ice:outputText value="#{msgs['logicaldoc-workflow.editor_template_load']}"/>
						</ice:commandLink>
					</ice:panelGroup>
					
					
						
								<ice:panelGroup styleClass="rightToolSite"  style="padding-right:10px;" rendered="#{not empty WorkflowTemplateManager.persistenceTemplate}">
									<ice:commandLink action="#{WorkflowTemplateManager.saveCurrentWorkflowTemplate}">
										<ice:graphicImage styleClass="linkImage" value="/skins/default/images/workflow/editor/save.gif"/>
										<ice:outputText value="#{msgs['logicaldoc-workflow.editor_template_save']}"/>
									</ice:commandLink>
								</ice:panelGroup>
								
								<ice:panelGroup styleClass="deactivatedRightToolSite" rendered="#{empty WorkflowTemplateManager.persistenceTemplate}">
									<ice:graphicImage styleClass="linkImage" value="/skins/default/images/workflow/editor/save_dis.gif"/>
									<ice:outputText value="#{msgs['logicaldoc-workflow.editor_template_save']}"/>
								</ice:panelGroup>
									
								<ice:panelGroup styleClass="rightToolSite" rendered="#{not empty WorkflowTemplateManager.persistenceTemplate.id}">
									<ice:commandLink action="#{WorkflowTemplateManager.deployWorkflowTemplate}">
