			<ice:panelGrid width="100%" cellpadding="0" cellspacing="0" columns="2" style="text-align: center;">
                <ice:outputText style="color: #FFFFFF;" value="#{msgs['logicaldoc-workflow.editor_errors']}"/>
           		<ice:commandButton image="/skins/default/images/workflow/editor/close.gif" action="#{WorkflowTemplateManager.closeErrorMessageWindow}"/>
            </ice:panelGrid>
			
		</f:facet>

		<f:facet name="body">
        			<h:outputText value="#{msgs['logicaldoc-workflow.editor_errors_text']}"/>
        			<br />
		
			
				<ui:repeat value="#{WorkflowTemplateManager.errorMessages}"
					var="message">
	        				#{message.model.name}: #{message.message} <br />
				</ui:repeat>
			
		</f:facet>
		
	</ice:panelPopup>

