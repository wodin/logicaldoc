	xmlns:ldoc="http://www.logicaldoc.com/jsf" jsfc="ice:form">
	
	<script language="javascript" type="text/javascript" src="/js/workflow.js"/>


	<ice:outputStyle href="/skins/default/css/workflow.css" />


	<ice:panelPopup modal="true" clientOnly="true"
		
		draggable="false" visible="#{not empty WorkflowTemplateManager.errorMessages}">
