													<ice:commandLink action="#{WorkflowTemplateManager.selectedComponent.controller.addTransition}">
														<ice:graphicImage styleClass="symbol" value="/skins/default/images/workflow/editor/transition_add.gif"/>
														<ice:outputText value="#{msgs['logicaldoc-workflow.editor_component_task_addtransition']}"/>
													</ice:commandLink>
												
												</ice:panelCollapsible>
											</ice:panelGroup>		
											
											<!-- ****** -->
											<!--  Join  -->
											<!-- ****** -->	
											
											<ice:panelGroup rendered="#{not empty WorkflowTemplateManager.selectedComponent and WorkflowTemplateManager.selectedComponent.template == 'join'}">
												<ice:panelCollapsible expanded="true">
