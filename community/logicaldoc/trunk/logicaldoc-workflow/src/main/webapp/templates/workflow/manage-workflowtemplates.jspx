																			draggable="true"
																			dragMask="hover_start, hover_end"
																			dropTarget="true"
																			
																			dragListener="#{WorkflowTemplateManager.dragObject}"
																			dragValue="#{workflowComponent}"
																			dragOptions="dragGhost" dropMask="hover_start"
																			hoverclass="seriesHover" >
																				
																				<ice:panelGroup rendered="#{empty workflowComponent.name}">
																					<i>#{msgs['logicaldoc-workflow.editor_aa_noname']}</i>
																				</ice:panelGroup>
																				<ice:panelGroup rendered="#{not empty workflowComponent.name}">
																					
																					#{workflowComponent.name}
							
																				</ice:panelGroup>
																			
																			
																				<ice:commandLink onclick="if(window.confirm('Do you really want to to delete this WorkflowComponent?') == 0)return false;" actionListener="#{WorkflowTemplateManager.deleteWorkflowComponent}"  value="#{msgs['logicaldoc-workflow.editor_aa_delete']}">
																					<f:param name="component" value="#{workflowComponent}"/>
																				</ice:commandLink>
																					
																			<ice:commandLink actionListener="#{WorkflowTemplateManager.selectComponent}" value="#{msgs['logicaldoc-workflow.editor_aa_edit']}">
																				<f:param name="component" value="#{workflowComponent}"/>
																			</ice:commandLink>
																	</ice:panelGroup>
																</td>
															</tr>
														</table>
