								</ice:panelGroup>
									
								<ice:panelGroup styleClass="rightToolSite" rendered="#{not empty WorkflowTemplateManager.persistenceTemplate.id}">
									<ice:commandLink action="#{WorkflowTemplateManager.deleteWorkflowTemplate}">
										<ice:graphicImage styleClass="linkImage" value="/skins/default/images/workflow/editor/delete.gif"/>
										<ice:outputText value="#{msgs['logicaldoc-workflow.editor_template_remove']}"/>
									</ice:commandLink>
								</ice:panelGroup>
								
								<ice:panelGroup styleClass="deactivatedRightToolSite" rendered="#{ empty WorkflowTemplateManager.persistenceTemplate.id}">
									<ice:graphicImage styleClass="linkImage" value="/skins/default/images/workflow/editor/delete_dis.gif"/>
									<ice:outputText value="#{msgs['logicaldoc-workflow.editor_template_remove']}"/>
								</ice:panelGroup>
								
									
								<ice:panelGroup styleClass="rightToolSite" rendered="#{not empty WorkflowTemplateManager.persistenceTemplate}">
									<ice:commandLink action="#{WorkflowTemplateManager.closeCurrentWorkflowTemplate}">
										<ice:graphicImage styleClass="linkImage" value="/skins/default/images/workflow/editor/template_close.gif"/>
										<ice:outputText value="#{msgs['logicaldoc-workflow.editor_template_close']}"/>
									</ice:commandLink>
								</ice:panelGroup>
								
								<ice:panelGroup styleClass="deactivatedRightToolSite" rendered="#{ empty WorkflowTemplateManager.persistenceTemplate}">
									<ice:graphicImage styleClass="linkImage" value="/skins/default/images/workflow/editor/template_close_dis.gif"/>
									<ice:outputText value="#{msgs['logicaldoc-workflow.editor_template_close']}"/>
								</ice:panelGroup>
					
					
				</ice:panelGroup>
				
			</td>
		</tr>
		<tr>
			<td colspan="3" height="22"  valign="top" bgcolor="#C4DAEE">
				
				<ice:panelGroup rendered="#{not empty WorkflowTemplateManager.persistenceTemplate}">
			
					<ice:panelGroup styleClass="rightToolSite" style="margin-top:4px;">
						<ice:commandLink actionListener="#{WorkflowTemplateManager.addWorkflowComponent}"
							>
							<f:param name="workflowType" value="WorkflowTask"/>	
							<ice:graphicImage styleClass="linkImage" value="/skins/default/images/workflow/editor/menu_task.gif"/>
							<ice:outputText value="#{msgs['logicaldoc-workflow.editor_component_addtask']}"/>
						</ice:commandLink>
					</ice:panelGroup>
					<ice:panelGroup styleClass="rightToolSite"  style="margin-top:4px;">
						<ice:commandLink actionListener="#{WorkflowTemplateManager.addWorkflowComponent}"
							>
							<f:param name="workflowType" value="EndState"/>	
							<ice:graphicImage styleClass="linkImage" value="/skins/default/images/workflow/editor/menu_endstate.gif"/>
							<ice:outputText value="#{msgs['logicaldoc-workflow.editor_component_addendstate']}"/>
						</ice:commandLink>
					</ice:panelGroup>
					
					<ice:panelGroup styleClass="rightToolSite"  style="margin-top:4px;">
						<ice:commandLink actionListener="#{WorkflowTemplateManager.addWorkflowComponent}"
							>
							<f:param name="workflowType" value="Fork"/>	
							<ice:graphicImage styleClass="linkImage" value="/skins/default/images/workflow/editor/menu_fork.gif"/>
							<ice:outputText value="#{msgs['logicaldoc-workflow.editor_component_addfork']}"/>
						</ice:commandLink>
					</ice:panelGroup>	
					
					<ice:panelGroup styleClass="rightToolSite"  style="margin-top:4px;">
						<ice:commandLink actionListener="#{WorkflowTemplateManager.addWorkflowComponent}"
							>
							<f:param name="workflowType" value="Join"/>	
							<ice:graphicImage styleClass="linkImage" value="/skins/default/images/workflow/editor/menu_join.gif"/>
							<ice:outputText value="#{msgs['logicaldoc-workflow.editor_component_addjoin']}"/>
						</ice:commandLink>
					</ice:panelGroup>
					
					<ice:panelGrid columns="2">
						<h:outputText value="#{editor_statstateeditor_statstate}"/>
						<h:selectOneMenu value="#{WorkflowTemplateManager.workflowTemplate.startState}">
							<f:selectItems value="#{WorkflowTemplateManager.possibleStartStates}"/>
						</h:selectOneMenu>
					</ice:panelGrid>
				
				</ice:panelGroup>
			</td>
		</tr>
		
	</table>	
	
	
	
				<ice:panelGroup rendered="#{not empty WorkflowTemplateManager.persistenceTemplate}" style="background-image:url('/skins/default/images/workflow/editor/editor_gradient.jpg'); background-repeat:repeat-x"> 
					
<!-- ************************************************************************ -->
<!-- START - Following code should be migrated into a single jspx file - START-->
<!-- ************************************************************************ -->

				<ice:panelGrid columns="3" rendered="#{not empty WorkflowTemplateManager.persistenceTemplate}">
