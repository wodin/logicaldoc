														<ice:outputLabel for="name" value="#{msgs['logicaldoc-workflow.editor_component_join_name']}"><ice:inputText id="name" value="#{WorkflowTemplateManager.selectedComponent.name}"/></ice:outputLabel>
														
														
													</ice:panelGrid>	
												</ice:panelCollapsible>
											</ice:panelGroup>
											
											<!-- ****** -->
											<!--  Fork  -->
											<!-- ****** -->	
											
											<ice:panelGroup rendered="#{not empty WorkflowTemplateManager.selectedComponent and WorkflowTemplateManager.selectedComponent.template == 'fork'}">
												<ice:panelCollapsible expanded="true">
													<f:facet name="header">
										                <ice:panelGroup>
										                    <ice:outputText value="#{msgs['logicaldoc-workflow.editor_component_fork']}"/>
										                </ice:panelGroup>
										            </f:facet>
										           	<ice:panelGrid columnClasses="hightAlignment" cellpadding="0" cellspacing="0" columns="2" rendered="#{ WorkflowTemplateManager.selectedComponent != null}">
														<ice:outputLabel for="name" value="#{msgs['logicaldoc-workflow.editor_component_fork_name']}"><ice:inputText id="name" value="#{WorkflowTemplateManager.selectedComponent.name}"/></ice:outputLabel>	
													</ice:panelGrid>
										        </ice:panelCollapsible>
													
											</ice:panelGroup>
											
											
