										            </f:facet>
										            <ice:panelGrid columnClasses="hightAlignment" cellpadding="0" cellspacing="0" columns="2" rendered="#{ WorkflowTemplateManager.selectedComponent != null}">

												    	<ice:outputLabel for="name" value="#{editor_component_endstate_name}"><ice:inputText id="name" value="#{WorkflowTemplateManager.selectedComponent.name}"/></ice:outputLabel>
													
													</ice:panelGrid>
										        </ice:panelCollapsible>
													
											</ice:panelGroup>
