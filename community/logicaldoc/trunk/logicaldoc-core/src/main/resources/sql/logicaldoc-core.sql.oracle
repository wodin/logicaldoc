create table ld_bookmark (ld_id number(19,0) not null, ld_lastmodified timestamp not null, ld_deleted int not null, ld_userid number(19,0) not null, ld_docid number(19,0) not null, ld_title varchar(255) not null, ld_description varchar(4000), ld_position int not null, ld_filetype varchar(40), ld_type int not null, primary key (ld_id));
create table ld_document (ld_id number(19,0) not null, ld_lastmodified timestamp not null, ld_deleted int not null, ld_immutable int not null,
                          ld_customid varchar(255), ld_title varchar(255), ld_version varchar(10), ld_fileversion varchar(10), ld_date timestamp, 
                          ld_creation timestamp not null, ld_publisher varchar(255), ld_publisherid number(19,0) not null, ld_creator varchar(255),
                          ld_creatorid number(19,0) not null, ld_status int, ld_type varchar(255), ld_lockuserid number(19,0), ld_source varchar(4000),
                          ld_sourceauthor varchar(255), ld_sourcedate timestamp null, ld_sourceid varchar(1000), ld_sourcetype varchar(255),
                          ld_object varchar(1000), ld_coverage varchar(255), ld_language varchar(10), ld_filename varchar(255), 
                          ld_filesize number(19,0), ld_indexed int not null, ld_barcoded int not null, ld_signed int not null, ld_digest varchar(255),
                          ld_recipient varchar(1000), ld_folderid number(19,0), ld_templateid number(19,0), ld_exportstatus int not null, ld_exportid number(19,0),
                          ld_exportname varchar(255), ld_exportversion varchar(10), ld_docref number(19,0), ld_deleteuserid number(19,0), ld_rating int,
                          ld_comment varchar(1000), ld_workflowstatus varchar(1000), ld_published int not null, ld_startpublishing timestamp,
                          ld_stoppublishing timestamp null, ld_transactionid varchar(255), ld_extresid varchar(255), ld_tgs varchar(1000), primary key (ld_id));
create table ld_document_ext (ld_docid number(19,0) not null, ld_mandatory int not null, ld_type int not null, ld_editor int not null, ld_position int not null, ld_stringvalue varchar(4000), ld_intvalue number(19,0), ld_doublevalue float, ld_datevalue timestamp null, ld_name varchar(255) not null, ld_label varchar(255), primary key (ld_docid, ld_name));
create table ld_generic (ld_id number(19,0) not null, ld_lastmodified timestamp not null, ld_deleted int not null, ld_type varchar(255) not null, ld_subtype varchar(255) not null, ld_qualifier number(19,0) null, ld_string1 varchar(4000), ld_string2 varchar(4000), ld_string3 varchar(4000), ld_integer1 number(19,0) null, ld_integer2 number(19,0) null, ld_integer3 number(19,0) null, ld_double1 float, ld_double2 float, ld_date1 timestamp null, ld_date2 timestamp null, primary key (ld_id));
create table ld_generic_ext (ld_genid number(19,0) not null, ld_mandatory int not null, ld_type int not null, ld_editor int not null, ld_position int not null, ld_stringvalue varchar(4000), ld_intvalue number(19,0), ld_doublevalue float, ld_datevalue timestamp null, ld_name varchar(255) not null, ld_label varchar(255), primary key (ld_genid, ld_name));
create table ld_group (ld_id number(19,0) not null, ld_lastmodified timestamp not null, ld_deleted int not null, ld_name varchar(255) not null, ld_description varchar(255), ld_type int not null, primary key (ld_id));
create table ld_group_ext (ld_groupid number(19,0) not null, ld_mandatory int not null, ld_type int not null, ld_editor int not null, ld_position int not null, ld_stringvalue varchar(4000), ld_intvalue number(19,0), ld_doublevalue float, ld_datevalue timestamp null, ld_name varchar(255) not null, ld_label varchar(255), primary key (ld_groupid, ld_name));
create table ld_history (ld_id number(19,0) not null, ld_lastmodified timestamp not null, ld_deleted int not null, ld_docid number(19,0), ld_folderid number(19,0) not null, ld_userid number(19,0), ld_date timestamp, ld_username varchar(255), ld_event varchar(255), ld_comment varchar(4000), ld_version varchar(10), ld_title varchar(255), ld_path varchar(4000), ld_notified int not null, ld_sessionid varchar(255), ld_new int, ld_filename varchar(255), primary key (ld_id));
create table ld_tag (ld_docid number(19,0), ld_tag varchar(255));
create table ld_link (ld_id number(19,0) not null, ld_lastmodified timestamp not null, ld_deleted int not null, ld_type varchar(255) not null, ld_docid1 number(19,0), ld_docid2 number(19,0), primary key (ld_id));
create table ld_menu (ld_id number(19,0) not null, ld_lastmodified timestamp not null, ld_deleted int not null, ld_text varchar(255), ld_parentid number(19,0) not null, ld_securityref number(19,0), ld_icon varchar(255), ld_type int not null, ld_description varchar(4000), primary key (ld_id));
create table ld_menugroup (ld_menuid number(19,0) not null, ld_groupid number(19,0) not null, ld_write int not null, primary key (ld_menuid, ld_groupid, ld_write));
create table ld_recipient (ld_messageid number(19,0) not null, ld_name varchar(255) not null, ld_address varchar(255) not null, ld_mode varchar(255) not null, ld_type int not null, ld_read int not null);
create table ld_systemmessage (ld_id number(19,0) not null, ld_lastmodified timestamp not null, ld_deleted int not null, 
                               ld_author varchar(255), ld_messagetext varchar(2000), ld_subject varchar(255), 
                               ld_sentdate timestamp not null, ld_datescope int, ld_prio int, ld_confirmation int, 
                               ld_lastnotified timestamp, ld_status int not null, ld_trials int, ld_type int not null, primary key (ld_id));
create table ld_template (ld_id number(19,0) not null, ld_lastmodified timestamp not null, ld_deleted int not null, ld_name varchar(255) not null, ld_description varchar(2000), ld_readonly int not null, ld_type int not null, ld_category int not null, ld_signrequired int not null, ld_retentiondays int null, primary key (ld_id));
create table ld_template_ext (ld_templateid number(19,0) not null, ld_mandatory int not null, ld_type int not null, ld_editor int not null, ld_position int not null, ld_stringvalue varchar(4000), ld_intvalue number(19,0), ld_doublevalue float, ld_datevalue timestamp null, ld_name varchar(255) not null, ld_label varchar(255), primary key (ld_templateid, ld_name));
create table ld_ticket (ld_id number(19,0) not null, ld_lastmodified timestamp not null, ld_deleted int not null, ld_ticketid varchar(255) not null, ld_docid number(19,0) not null, ld_userid number(19,0) not null, ld_type int not null, ld_creation timestamp not null, ld_expired timestamp, ld_count int not null, primary key (ld_id));
create table ld_user (ld_id number(19,0) not null, ld_lastmodified timestamp not null, ld_deleted int not null, ld_enabled int not null, 
                      ld_username varchar(255) not null, ld_password varchar(255), ld_name varchar(255), ld_firstname varchar(255), 
                      ld_street varchar(255), ld_postalcode varchar(255), ld_city varchar(255), ld_country varchar(255), 
                      ld_state varchar(255), ld_language varchar(10), ld_email varchar(255), ld_telephone varchar(255), 
                      ld_telephone2 varchar(255), ld_type int not null, ld_passwordchanged timestamp, ld_passwordexpires int not null,
                      ld_source int not null, ld_quota number(19,0) not null, ld_quotacount number(19,0) not null, ld_signatureid varchar(255), 
                      ld_signatureinfo varchar(255), ld_welcomescreen int null, ld_ipwhitelist varchar(4000), 
                      ld_ipblacklist varchar(4000), ld_passwordexpired int not null,  primary key (ld_id));
create table ld_user_history (ld_id number(19,0) not null, ld_lastmodified timestamp not null, ld_deleted int not null, ld_userid number(19,0), ld_date timestamp, ld_username varchar(255), ld_event varchar(255), ld_comment varchar(4000), ld_notified int not null, ld_sessionid varchar(255), ld_new int, ld_filename varchar(255), primary key (ld_id));
create table ld_userdoc (ld_id number(19,0) not null, ld_lastmodified timestamp not null, ld_deleted int not null, ld_docid number(19,0) not null, ld_userid number(19,0) not null, ld_date timestamp, primary key (ld_id));
create table ld_usergroup (ld_groupid number(19,0) not null, ld_userid number(19,0) not null, primary key (ld_groupid, ld_userid));
create table ld_version (ld_id number(19,0) not null, ld_lastmodified timestamp not null, ld_deleted int not null, ld_immutable int not null, ld_customid varchar(255),
                         ld_title varchar(255), ld_version varchar(10), ld_fileversion varchar(10), ld_date timestamp, ld_creation timestamp, ld_publisher varchar(255),
                         ld_publisherid number(19,0) not null,  ld_creator varchar(255), ld_creatorid number(19,0) not null, ld_status int, ld_type varchar(255),
                         ld_lockuserid number(19,0), ld_source varchar(4000), ld_sourceauthor varchar(255), ld_sourcedate timestamp null, ld_sourceid varchar(1000),
                         ld_sourcetype varchar(255), ld_object varchar(1000), ld_coverage varchar(255), ld_language varchar(10), ld_filename varchar(255), 
                         ld_filesize number(19,0), ld_indexed int not null, ld_barcoded int not null, ld_signed int not null, ld_digest varchar(255),
                         ld_recipient varchar(1000), ld_folderid number(19,0), ld_foldername varchar(1000), ld_templateid number(19,0), ld_templatename varchar(1000),
                         ld_tgs varchar(1000), ld_username varchar(255), ld_userid number(19,0), ld_versiondate timestamp, ld_comment varchar(1000),ld_event varchar(255),
                         ld_documentid number(19,0) not null, ld_exportstatus int not null, ld_exportid number(19,0), ld_exportname varchar(255), ld_exportversion varchar(10),
                         ld_deleteuserid number(19,0), ld_workflowstatus varchar(1000), ld_published int not null, ld_startpublishing timestamp,
                         ld_stoppublishing timestamp null, ld_transactionid varchar(255), ld_extresid varchar(255),primary key (ld_id));
create table ld_version_ext (ld_versionid number(19,0) not null, ld_mandatory int not null, ld_type int not null, ld_editor int not null, ld_position int not null, ld_stringvalue varchar(4000), ld_intvalue number(19,0), ld_doublevalue float, ld_datevalue timestamp null, ld_name varchar(255) not null, ld_label varchar(255), primary key (ld_versionid, ld_name));
create table ld_folder(ld_id number(19,0) not null, ld_lastmodified timestamp not null, ld_deleted int not null, ld_name varchar(255), ld_parentid number(19,0) not null, ld_securityref number(19,0), ld_description varchar(4000), ld_type int not null, ld_creation timestamp, ld_creator varchar(255), ld_creatorid number(19,0), ld_templateid number(19,0), ld_templocked int not null, primary key (ld_id));
create table ld_folder_ext (ld_folderid number(19,0) not null, ld_mandatory int not null, ld_type int not null, ld_editor int not null, ld_position int not null, ld_stringvalue varchar(4000), ld_intvalue number(19,0), ld_doublevalue float, ld_datevalue timestamp null, ld_name varchar(255) not null, ld_label varchar(255), primary key (ld_folderid, ld_name));
create table ld_folder_history (ld_id number(19,0) not null, ld_lastmodified timestamp not null, ld_deleted int not null, ld_docid number(19,0), ld_folderid number(19,0) not null, ld_userid number(19,0), ld_date timestamp, ld_username varchar(255), ld_event varchar(255), ld_comment varchar(4000), ld_version varchar(10), ld_title varchar(255), ld_path varchar(4000), ld_notified int not null, ld_sessionid varchar(255), ld_new int, ld_filename varchar(255), primary key (ld_id));
create table ld_foldergroup (ld_folderid number(19,0) not null, ld_groupid number(19,0) not null, ld_write int not null, 
                             ld_add int not null, ld_security int not null, ld_immutable int not null, ld_delete int not null, 
                             ld_rename int not null, ld_import int not null, ld_export int not null, ld_sign int not null, 
                             ld_archive int not null, ld_workflow int not null, ld_download int not null, ld_calendar int not null,  
                             primary key (ld_folderid, ld_groupid, ld_write, ld_add, ld_security, ld_immutable, ld_delete, ld_rename, ld_import, ld_export, ld_sign, ld_archive, ld_workflow, ld_download, ld_calendar));
create table ld_feedmessage (ld_id number(19,0) not null, ld_lastmodified timestamp not null, ld_deleted int not null, ld_guid varchar(512) null, ld_title varchar(512) null, ld_description  varchar(4000) null, ld_link varchar(512) null, ld_pubdate timestamp, ld_read int not null, primary key (ld_id));
create table ld_rating (ld_id number(19,0) not null, ld_lastmodified timestamp not null, ld_deleted int not null, ld_docid number(19,0) not null, ld_userid number(19,0) not null, ld_vote int not null, primary key (ld_id));
create table ld_note (ld_id number(19,0) not null, ld_lastmodified timestamp not null, ld_deleted int not null, ld_docid number(19,0) not null, ld_username varchar(255), ld_userid number(19,0), ld_date timestamp, ld_message varchar(4000), primary key (ld_id));
create table ld_messagetemplate (ld_id number(19,0) not null, ld_lastmodified timestamp not null, 
                                 ld_deleted int not null, ld_name varchar(255) not null, ld_language varchar(10) not null,
                                 ld_description varchar(1000), ld_body varchar(4000),
                                 ld_subject varchar(1000), primary key (ld_id));

